# ==========================================
# Database Configuration
# ==========================================
# The name of your local database
POSTGRES_DB_NAME=teleinformatics_db

# Your local database username
POSTGRES_USER=username

# Your database password
POSTGRES_PASSWORD=your_password_here

# The port where your database is running (Default is 5432)
DB_PORT=5432

# ==========================================
# Security Configuration (JWT)
# ==========================================
JWT_SECRET=change_this_to_a_secure_random_secret_key_at_least_32_chars_long

# Token expiration time in milliseconds
# Examples: 
# 3600000 = 1 hour
# 8640000 = 2.4 hours
# 86400000 = 24 hours
JWT_EXPIRATION=8640000